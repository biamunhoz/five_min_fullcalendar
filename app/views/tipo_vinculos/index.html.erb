<p id="notice"><%= notice %></p>

<h1>Vínculos</h1>

<table>
  <thead>
    <tr>
      <th>Vínculo</th>
      <th>Cód. do setor</th>
      <th>Nome abreviado do setor</th>
      <th>Nome do setor</th>
      <th>Cód. Unidade</th>
      <th>Sigla</th>
      <th>Unidade</th>
      <th>Vínculo</th>
      <th>Função</th>
      <th>Usuário</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_vinculos.each do |tipo_vinculo| %>
      <tr>
        <td><%= tipo_vinculo.tipoVinculo %></td>
        <td><%= tipo_vinculo.codigoSetor %></td>
        <td><%= tipo_vinculo.nomeAbreviadSetor %></td>
        <td><%= tipo_vinculo.nomeSetor %></td>
        <td><%= tipo_vinculo.codigoUnidade %></td>
        <td><%= tipo_vinculo.siglaUnidade %></td>
        <td><%= tipo_vinculo.nomeUnidade %></td>
        <td><%= tipo_vinculo.nomeVinculo %></td>
        <td><%= tipo_vinculo.nomeAbreviadoFuncao %></td>
        <td><%= tipo_vinculo.usuario %></td>
        <td><%= link_to 'Mostrar', tipo_vinculo %></td>
        <td><%= link_to 'Editar', edit_tipo_vinculo_path(tipo_vinculo) %></td>
        <td><%= link_to 'Apagar', tipo_vinculo, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

