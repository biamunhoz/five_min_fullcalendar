<%= link_to 'Novo Evento', new_event_path, class: 'btn btn-info'  %>
<br>
<br>

<% @dadosagenda.each do |agenda| %>
    <p><b><%= agenda.nome %></b> - <%= agenda.observacao %> </p> 
<% end %>


<table>
