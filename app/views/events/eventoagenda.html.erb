        <% @salasdaagenda.each do |sala|  %>            
            <td><input type="color" id="favcolor" name="favcolor" disabled value="<%= sala.cor %>">  <%= sala.nome %> </td>
        <% end %>
    </tr>
</table>



<div id="calendario"></div>